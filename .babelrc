{
  "presets": [
    ["@babel/preset-env", {
      "targets": { "node": 10 },
      "useBuiltIns": 'usage',
      "corejs":3
    }],
    "@babel/preset-react"
  ],
  "plugins": ["add-module-exports", "dynamic-import-webpack"],
  "env": {
    "production": {
      "presets": ["react-optimize"],
      "plugins": ["babel-plugin-dev-expression"]
    },
    "development": {
      "plugins": [
        "@babel/plugin-proposal-class-properties",
        "@babel/plugin-transform-classes"
      ]
    }
  }
}